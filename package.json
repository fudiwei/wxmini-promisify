        "Weixin",
        "MicroMessager",
        "MiniProgram",
        "MiniGame",
        "Wechat-MiniProgram",
        "Wechat-MiniGame",
        "Weixin-MiniProgram",
        "Weixin-MiniGame",
        "weapp",
        "wxapp",
        "promise",
        "promisify"
    ],
    "version": "2.19.4",
    "main": "dist/index.js",
    "author": "Fu Diwei",
    "repository": {
        "type": "git",
        "url": "https://github.com/fudiwei/wxmini-promisify.git"
    },
    "license": "MIT",
    "scripts": {
        "clean": "gulp clean",
        "eslint": "gulp eslint",
        "build": "gulp dist --sourceMap=no",
        "deploy": "npm run build && npm publish --access=public"
    },
    "typings": "types/index.d.ts",
    "files": [
        "dist/*",
        "types/*"
